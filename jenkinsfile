pipeline{
    agent any
    stages{
        stage('install'){
            steps{
                sh 'npm install'
            }

        }
        stage('package'){
            steps{
                sh 'npm run build --prod'
            }

        }
        stage('deploy'){
            steps{
                sh 'cp -r dist/jeeven-app/*  /var/www/html/'
            }

        }
    }
}